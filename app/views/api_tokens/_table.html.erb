<table id="api-token-table" class="table tablesorter custom-popup">
  <thead>
    <tr>
      <th>Token</th>
      <th>Name</th>
      <th>Description</th>
      <th>Owner</th>
      <th>Read</th>
      <th>Write</th>
      <th>Maintenance Logs</th>
      <th>Cloud Reports</th>
    </tr>
  </thead>
  <tbody>
    <%- api_tokens.each do |api_token| -%>
      <%- api_token = k(:api_token, api_token) -%>
      <tr id="#api-token-<%= api_token.id %>">
        <td><%= api_token.token_link %></td>
        <td><%= api_token.name %></td>
        <td><%= api_token.description %></td>
        <%- owner = k(:owner, api_token.owner) -%>
        <td><%= owner.name_link %></td>
        <td><%= api_token.read %></td>
        <td><%= api_token.write %></td>
        <td><%= api_token.post_logs %></td>
        <td><%= api_token.post_reports %></td>
      </tr>
      <%- end -%>
  </tbody>
</table>
